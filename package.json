    "nx": "22.3.3",
    "prettier": "3.7.4",
    "typedoc": "0.28.15",
    "typescript": "5.9.3",
    "typescript-eslint": "8.51.0",
    "vite": "7.3.0",
    "vite-plugin-dts": "4.5.4",
    "vitest": "4.0.16"
