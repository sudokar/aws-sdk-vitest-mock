    "@aws-sdk/types": "3.957.0",
    "@eslint/js": "9.39.2",
    "@nx/eslint": "22.3.3",
    "@nx/eslint-plugin": "22.3.3",
    "@nx/js": "22.3.3",
    "@nx/vite": "22.3.3",
    "@nx/vitest": "22.3.3",
    "@nx/workspace": "22.3.3",
    "@smithy/types": "4.11.0",
    "@swc-node/register": "1.11.1",
    "@swc/core": "1.15.8",
    "@swc/helpers": "0.5.18",
    "@types/eslint-plugin-security": "3.0.0",
    "@types/node": "24.10.4",
    "@typescript-eslint/parser": "8.51.0",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/eslint-plugin": "1.6.5",
    "eslint": "9.39.2",
